<Window x:Class="BlockBadWifi.DebugWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:BlockBadWifi"
        mc:Ignorable="d"
        Title="DebugWindow" Height="450" Width="600"
        Activated="Window_Activated">
    <Grid>
        <TextBox x:Name="textBox" Margin="10,39,10,10" TextWrapping="Wrap" Text="" ScrollViewer.CanContentScroll="True" VerticalScrollBarVisibility="Auto"/>
        <Button x:Name="button" Content="Refresh" HorizontalAlignment="Left" Height="24" Margin="10,10,0,0" VerticalAlignment="Top" Width="114" Click="Button_Click"/>

    </Grid>
</Window>
